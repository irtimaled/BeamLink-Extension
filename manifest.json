{
	"manifest_version": 2,
	"name": "BeamLink Extension",
	"description": "Makes BeamLink's messages look better",
	"version": "1.3",
	"icons": {"256": "icon.png"},
	
	"content_scripts": [{
		"matches": ["*://*.beam.pro/*"],
		"js": ["jquery-1.11.3.min.js", "beam.js"]
	}, {
		"matches": ["*://*.twitch.tv/*"],
		"js": ["jquery-1.11.3.min.js", "twitch.js"]
	}]
}
