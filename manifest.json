{
	"manifest_version": 2,
	"name": "BeamLink Extension",
	"description": "Makes BeamLink's messages on Twitch and Beam look nicer.",
	"version": "1.2",
	"icons": {"256": "icon.png"},
	
	"content_scripts": [{
		"matches": ["*://*.beam.pro/*"],
		"js": ["jquery-1.11.2.min.js", "beam.js"]
	}, {
		"matches": ["*://*.twitch.tv/*"],
		"js": ["jquery-1.11.2.min.js", "twitch.js"]
	}]
}
